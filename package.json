{
  "name": "dojo-timer",
  "version": "1.0.0",
  "description": "dojo timer for the win",
  "main": "electron/electron.js",
  "license": "MIT",
  "extends": null,
  "homepage": "./",
  "scripts": {
    "dev": "concurrently \"BROWSER=none yarn start\" \"wait-on http://localhost:8080 && electron .\"",
    "start": "webpack-dev-server --mode development --config config/webpack.dev.config.js --hot",
    "build": "webpack --mode production --config config/webpack.prod.config.js --env.NODE_ENV=production --progress",
    "electron-pack": "electron-builder",
    "postinstall": "electron-rebuild",
    "preelectron-pack": "yarn postinstall && yarn build ",
    "lint": "yarn lint:js && yarn lint:scss",
    "lint:js": "eslint src/**/*.js",
    "lint:scss": "npx stylelint src/**/*.scss",
    "postelectron-pack": "electron-builder install-app-deps"
  },
  "build": {
    "linux": {
      "target": "deb",
      "maintainer": "Gustavo Freire",
      "category": "Development",
      "icon" : "icons/icon.png"
    },
    "appId": "com.example.electron-cra",
    "files": [
      "electron/electron.js",
      "dist/main.js",
      "dist/style.css",
      "dist/index.html",
      "timer.png"
    ],
    "directories": {
      "buildResources": "resources",
      "output": "electron-build"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.7.7",
    "babel-loader": "^8.0.6",
    "concurrently": "^5.1.0",
    "css-loader": "^3.4.2",
    "electron": "^8.0.0",
    "electron-builder": "^22.3.2",
    "electron-installer-snap": "^5.0.0",
    "electron-rebuild": "^1.10.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.10.0",
    "eslint-import-resolver-webpack": "^0.12.1",
    "eslint-plugin-flowtype": "^4.6.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^5.0.2",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^6.0.0",
    "node-sass": "^7.0.0",
    "optimize-css-assets-webpack-plugin": "^5.0.0",
    "prettier": "^1.19.1",
    "react-redux": "^7.1.3",
    "redux": "^4.0.5",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "stylelint": "^13.0.0",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-order": "^4.0.0",
    "stylelint-prettier": "^1.1.2",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^3.0.0",
    "wait-on": "^4.0.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.1.5"
  },
  "dependencies": {
    "child_process": "^1.0.2",
    "electron-is-dev": "^1.0.1",
    "fs": "0.0.2",
    "moment": "^2.24.0",
    "net": "^1.0.2",
    "node-pty": "^0.9.0",
    "os": "^0.1.1",
    "path": "^0.12.7",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "webpack-node-externals": "^1.7.2",
    "xterm": "^3.14.5",
    "xterm-addon-fit": "^0.3.0"
  }
}
